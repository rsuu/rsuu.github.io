&lt;&#x2F;span&gt;&lt;span&gt;f(1, 2, enum e::Unnamed(1));
&lt;&#x2F;span&gt;&lt;span&gt;f(1, 2, enum e::Named { r: &lt;&#x2F;span&gt;&lt;span style=&quot;color:#d3869b;&quot;&gt;1 &lt;&#x2F;span&gt;&lt;span&gt;} );
